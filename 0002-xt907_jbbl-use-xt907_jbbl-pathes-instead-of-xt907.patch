From 3c1613d6ad28a830d3a0feb3256a22cc56cc4aec Mon Sep 17 00:00:00 2001
From: Den <motometalor@gmail.com>
Date: Sat, 26 Jan 2019 12:39:57 +0300
Subject: [PATCH 2/3] xt907_jbbl: use xt907_jbbl pathes instead of xt907

According to rebranding xt907 -> to xt907_jbbl
---
 xt907_jbbl/BoardConfigVendor.mk       | 2 +-
 xt907_jbbl/xt907_jbbl-vendor-blobs.mk | 6 +++---
 xt907_jbbl/xt907_jbbl-vendor.mk       | 4 ++--
 3 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/xt907_jbbl/BoardConfigVendor.mk b/xt907_jbbl/BoardConfigVendor.mk
index 247fa0db..c5e53e39 100644
--- a/xt907_jbbl/BoardConfigVendor.mk
+++ b/xt907_jbbl/BoardConfigVendor.mk
@@ -12,4 +12,4 @@
 # See the License for the specific language governing permissions and
 # limitations under the License.
 
-# This file is generated by device/motorola/xt907/setup-makefiles.sh
+# This file is generated by device/motorola/xt907_jbbl/setup-makefiles.sh
diff --git a/xt907_jbbl/xt907_jbbl-vendor-blobs.mk b/xt907_jbbl/xt907_jbbl-vendor-blobs.mk
index 2b4e0d7c..69a56d58 100644
--- a/xt907_jbbl/xt907_jbbl-vendor-blobs.mk
+++ b/xt907_jbbl/xt907_jbbl-vendor-blobs.mk
@@ -12,8 +12,8 @@
 # See the License for the specific language governing permissions and
 # limitations under the License.
 
-# This file is generated by device/motorola/xt907/setup-makefiles.sh
+# This file is generated by device/motorola/xt907_jbbl/setup-makefiles.sh
 
 PRODUCT_COPY_FILES += \
-    vendor/motorola/xt907/proprietary/etc/firmware/atmxt-r1.tdat:system/etc/firmware/atmxt-r1.tdat \
-    vendor/motorola/xt907/proprietary/etc/firmware/atmxt-r2.tdat:system/etc/firmware/atmxt-r2.tdat
+    vendor/motorola/xt907_jbbl/proprietary/etc/firmware/atmxt-r1.tdat:system/etc/firmware/atmxt-r1.tdat \
+    vendor/motorola/xt907_jbbl/proprietary/etc/firmware/atmxt-r2.tdat:system/etc/firmware/atmxt-r2.tdat
diff --git a/xt907_jbbl/xt907_jbbl-vendor.mk b/xt907_jbbl/xt907_jbbl-vendor.mk
index a5801c69..e75c0002 100644
--- a/xt907_jbbl/xt907_jbbl-vendor.mk
+++ b/xt907_jbbl/xt907_jbbl-vendor.mk
@@ -15,9 +15,9 @@
 # This file is generated by device/motorola/xt907/setup-makefiles.sh
 
 # Pick up overlay for features that depend on non-open-source files
-DEVICE_PACKAGE_OVERLAYS += vendor/motorola/xt907/overlay
+DEVICE_PACKAGE_OVERLAYS += vendor/motorola/xt907_jbbl/overlay
 
-$(call inherit-product, vendor/motorola/xt907/xt907-vendor-blobs.mk)
+$(call inherit-product, vendor/motorola/xt907_jbbl/xt907_jbbl-vendor-blobs.mk)
 $(call inherit-product, vendor/motorola/msm8960-common/msm8960-common-vendor.mk)
 $(call inherit-product, vendor/motorola/qcom-common/qcom-common-vendor.mk)
 
-- 
